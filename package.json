{
  "name": "mustache-i18n",
  "version": "1.0.0",
  "description": "i18n plugin for mustache.js.",
  "author": "David da Silva <dasilvacontin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dasilvacontin/mustache-i18n"
  },
  "bugs": "https://github.com/dasilvacontin/mustache-i18n/issues",
  "scripts": {
    "lint": "standard",
    "test": "node test/index.js | tap-spec",
    "test:watch": "gaze 'npm test' 'test/**/*.js' 'lib/**/*.js' 'index.js'",
    "start": "npm run test:watch",
    "istanbul": "istanbul cover test/index.js | tap-spec",
    "pretest": "npm run lint",
    "preistanbul": "npm run lint",
    "prepublish": "npm run test"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "mustache-i18n",
    "mustache",
    "i18n",
    "l10n",
    "internationalization",
    "localization",
    "plugin",
    "helper"
  ],
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.2",
    "gaze-cli": "^0.2.0",
    "istanbul": "^0.3.8",
    "mustache": "^2.1.3",
    "standard": "^4.5.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.1"
  }
}
